@model IList<Financeiro.Entidades.Usuario>

@Html.ActionLink("Novo Usuário", "Form")

<table class="table table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.id</td>
                <td>@usuario.nome</td>
                <td>@usuario.email</td>
            </tr>
        }
    </tbody>
</table>
